    "jwt-decode": "^2.2.0",
    "pm2": "^2.10.4",
    "prop-types": "^15.5.10",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-helmet": "5.2.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.1.2",
    "react-transition-group": "^2.5.3",
    "recompose": "^0.25.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.4",
    "uuid": "^3.2.1",
    "ws": "^5.1.1"
  },
  "devDependencies": {
