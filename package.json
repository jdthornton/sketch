{
  "name": "Sketch",
  "version": "1.0.0",
  "description": "Online pictionary application",
  "main": "index.js",
  "scripts": {
    "start": "nodemon build/server.js",
    "dev:client": "rimraf dist/js dist/css && cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --open --config webpack/webpack.client.config.js --port 8080",
    "dev:server": "rimraf build && cross-env NODE_ENV=development webpack --config webpack/webpack.server.config.js -w",
    "prod:server": "rimraf build && cross-env NODE_ENV=production webpack --config webpack/webpack.server.prod.config.js",
    "prod:client": "rimraf dist/js dist/css && cross-env NODE_ENV=production webpack --config webpack/webpack.client.prod.config.js",
    "prod": "npm-run-all -p prod:server prod:client -s start"
  },
  "keywords": [
    "react",
    "redux",
    "saga",
    "rr4",
    "ssr",
    "code-spliting"
  ],
  "author": "Jake Thornton",
  "license": "MIT",
  "dependencies": {
    "@loadable/component": "^5.2.2",
    "@loadable/server": "^5.2.2",
    "connected-react-router": "^6.2.2",
    "cors": "^2.8.5",
    "express": "^4.15.3",
    "express-static-gzip": "^1.1.3",
    "helmet": "3.9.0",
    "history": "^4.7.2",
